generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Skill {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  image       String?
  icon        String[]
  title       String
  description String
}

model Project {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  title       String
  image       String
  alt         String
  github      String
  domain      String
  description String
}
